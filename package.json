{
  "name": "@poc/mainapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android:release": "npx react-native run-android --variant=release",
    "android-clean-build": "cd android && ./gradlew clean && cd ..",
    "android-build-debug": "cd android && ./gradlew assembleDebug && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "test": "jest",
    "start": "react-native start",
    "ios": "npx react-native run-ios",
    "run:ios:rebuild": "yarn rebuild:ios && yarn ios",
    "run:ios:nodemodules": "yarn rebuild:nodemodules && yarn ios",
    "rebuild:ios": "yarn clean:nodemodules && yarn clean:ios && yarn build:ios",
    "clean:ios": "rm -rf ios/build && rm -rf ios/Pods && yarn clean:ios:pods",
    "clean:ios:pods": "cd ios && pod deintegrate && pod cache clean --all && cd ..",
    "build:ios": "yarn && cd ios/ && pod install && cd ..",
    "rebuild:nodemodules": "yarn clean:nodemodules && yarn build:nodemodules",
    "clean:nodemodules": "rm -rf node_modules && rm -f yarn.lock",
    "build:nodemodules": "yarn install",
    "run:android": "yarn rebuild:android && yarn android",
    "rebuild:android": "yarn clean:android && yarn build:android",
    "clean:android": "rm -rf android/app/build && rm -rf android/build && rm -rf android/.gradle && rm -rf android/build.gradle-e",
    "build:android": "yarn && npx jetifier",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rebuild": "yarn clean && yarn build",
    "clean": "yarn clean:nodemodules && yarn clean:android && yarn clean:ios",
    "build": "yarn && yarn build:android && yarn build:ios"
  },
  "dependencies": {
    "@poc/loan": "*",
    "@poc/payment": "*",
    "@poc/pix": "*",
    "@poc/templates": "*",
    "@poc/ui": "*",
    "@react-navigation/bottom-tabs": "^6.5.5",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@reduxjs/toolkit": "^1.9.3",
    "react": "18.2.0",
    "react-native": "0.71.2",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-svg": "^13.8.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-redux": "^8.0.5",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "detox": "^20.6.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.2.1",
    "lerna": "^6.4.1",
    "metro-babel-register": "^0.76.0",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.8.4",
    "react-test-renderer": "18.2.0",
    "typescript": "*"
  },
  "jest": {
    "preset": "react-native"
  },
  "workspaces": [
    "packages/*"
  ]
}
